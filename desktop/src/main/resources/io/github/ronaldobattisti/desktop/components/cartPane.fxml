<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.VBox?>
<HBox fx:id="root" alignment="CENTER" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.ronaldobattisti.desktop.controllers.CartPaneController">

    <VBox HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
        <TableView fx:id="cartTable" prefHeight="400.0" prefWidth="323.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="75.0" text="ID"/>
                <TableColumn fx:id="colName" prefWidth="138.0" text="Product"/>
                <TableColumn fx:id="colQty" prefWidth="87.0" text="Qty"/>
            </columns>
        </TableView>
    </VBox>
</HBox>
